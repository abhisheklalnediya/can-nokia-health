  "bugs": {
    "url": "https://github.com/abhisheklalnediya/can-nokia-health/issues"
  },
  "homepage": "https://github.com/abhisheklalnediya/can-nokia-health#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "btoa": "^1.1.2",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "lokijs": "^1.5.3",
    "moment": "^2.20.1",
    "query-string": "^5.1.0",
    "request": "^2.83.0",
    "uuid": "^3.2.1"
  },
